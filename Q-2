#include <iostream>

int main() {
    int arr[] = {10, 20, 30, 40, 50};  // Declare an array of integers
    int *ptr = arr;  // Pointer pointing to the first element of the array

    std::cout << "Array elements using pointer arithmetic: " << std::endl;

    for (int i = 0; i < 5; i++) {
        std::cout << *(ptr + i) << " ";  // Access elements using pointer arithmetic
    }

    std::cout << std::endl;
    return 0;
}
